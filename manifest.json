{
    "version": "8.2",
	
	"manifest_version": 2,

    "name": "Direct.Yandex Extender",

    "description": "Adds some buttons to Yandex.Direct",

    "permissions": [ "webNavigation", "tabs", "https://direct.yandex.ru/*", "http://direct.yandex.ru/*" ],

    "background": {
        "scripts": [ "background.js" ]
    },
    "page_action": {
        "default_icon": {
            "19": "i/38.png"
        },
        "default_title": "Direct.Extender"
    }
}
